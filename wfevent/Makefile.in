include @top_srcdir@/Makefile.inc

SOURCES = wfevent.c
OBJECTS = $(SOURCES:.c=.o)

all: @MAKEBINDIR@ @MAKEBINDIR@wfevent 

depend:
	@makedepend -- $(CFLAGS) -- $(SOURCES)

clean :
	@ $(RM) $(OBJECTS)
	@ $(RM) @MAKEBINDIR@wfevent

install:
	if [ ! -d @bindir@ ] ; then \
                mkdir @bindir@; \
                chmod 755 @bindir@; \
        fi;
	$(INSTALL) -m 755  @MAKEBINDIR@wfevent	@bindir@

@MAKEBINDIR@ :
	mkdir @MAKEBINDIR@

@MAKEBINDIR@wfevent : $(OBJECTS)
	$(CC) -o $@ $(CFLAGS) @CC_LDFLAGS@ $(OBJECTS)  -L@MAKESHLIBDIR@ -lTdiShr -lMdsShr -lTreeShr $(LIBS)
