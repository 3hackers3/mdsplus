  <verb name="SPAWN">

    <routine name="mdsdcl_spawn"/>

    <parameter name="p1" label="COMMAND" type="REST_OF_LINE"/>
    <qualifier name="CARRIAGE_CONTROL" defaulted="True"/>
    <qualifier name="CLI"/>
