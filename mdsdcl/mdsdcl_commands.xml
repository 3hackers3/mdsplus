    <qualifier name="PROCESS" required="True"/>
    <qualifier name="PROMPT" required="True"/>
    <qualifier name="SYMBOLS" defaulted="True"/>
    <qualifier name="WAIT" defaulted="True"/>

"  </verb>
  <verb name="TYPE">

